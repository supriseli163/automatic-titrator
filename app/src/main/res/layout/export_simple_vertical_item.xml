<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="30px"
    android:gravity="center_vertical"
    android:orientation="horizontal">

    <TextView
        android:id="@+id/export_svi_sort"
        android:layout_width="140px"
        android:layout_height="wrap_content"
        android:text="序号"
        android:textAlignment="center"
        android:textColor="@color/fontBlack1"
        android:textSize="22px" />

    <TextView
        android:id="@+id/export_svi_testname"
        android:layout_width="180px"
        android:layout_height="wrap_content"
        android:text="样品名称"
        android:textAlignment="center"
        android:textColor="@color/fontBlack1"
        android:textSize="22px" />

    <TextView
        android:id="@+id/export_svi_testid"
        android:layout_width="180px"
        android:layout_height="wrap_content"
        android:text="样品编号"
        android:textAlignment="center"
        android:textColor="@color/fontBlack1"
        android:textSize="22px" />

    <TextView
        android:id="@+id/export_svi_createdate"
        android:layout_width="140px"
        android:layout_height="wrap_content"
        android:text="日期"
        android:textAlignment="center"
        android:textColor="@color/fontBlack1"
        android:textSize="22px" />

    <TextView
        android:id="@+id/export_svi_createtime"
        android:layout_width="140px"
        android:layout_height="wrap_content"
        android:text="时间"
        android:textAlignment="center"
        android:textColor="@color/fontBlack1"
        android:textSize="22px" />

    <TextView
        android:id="@+id/export_svi_temperature"
        android:layout_width="100px"
        android:layout_height="wrap_content"
        android:text="温度"
        android:textAlignment="center"
        android:textColor="@color/fontBlack1"
        android:textSize="22px" />

    <TextView
        android:id="@+id/export_svi_resulttype"
        android:layout_width="120px"
        android:layout_height="wrap_content"
        android:text="结果类型"
        android:textAlignment="center"
        android:textColor="@color/fontBlack1"
        android:textSize="22px" />

    <TextView
        android:id="@+id/export_svi_result"
        android:layout_width="160px"
        android:layout_height="wrap_content"
        android:text="测试结果"
        android:textAlignment="center"
        android:textColor="@color/fontBlack1"
        android:textSize="22px" />

    <TextView
        android:id="@+id/export_svi_creator"
        android:layout_width="140px"
        android:layout_height="wrap_content"
        android:text="测试人"
        android:textAlignment="center"
        android:textColor="@color/fontBlack1"
        android:textSize="22px" />
</LinearLayout>
