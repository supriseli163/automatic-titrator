<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="40px"
    android:gravity="center_vertical"
    android:orientation="horizontal">

    <TextView
        android:id="@+id/export_shi_sort"
        android:layout_width="200px"
        android:layout_height="wrap_content"
        android:text="序号"
        android:textAlignment="center"
        android:textColor="@color/fontBlack1"
        android:textSize="32px" />

    <TextView
        android:id="@+id/export_shi_testname"
        android:layout_width="240px"
        android:layout_height="wrap_content"
        android:text="样品名称"
        android:textAlignment="center"
        android:textColor="@color/fontBlack1"
        android:textSize="32px" />

    <TextView
        android:id="@+id/export_shi_testid"
        android:layout_width="240px"
        android:layout_height="wrap_content"
        android:text="样品编号"
        android:textAlignment="center"
        android:textColor="@color/fontBlack1"
        android:textSize="32px" />

    <TextView
        android:id="@+id/export_shi_createdate"
        android:layout_width="180px"
        android:layout_height="wrap_content"
        android:text="日期"
        android:textAlignment="center"
        android:textColor="@color/fontBlack1"
        android:textSize="32px" />

    <TextView
        android:id="@+id/export_shi_createtime"
        android:layout_width="180px"
        android:layout_height="wrap_content"
        android:text="时间"
        android:textAlignment="center"
        android:textColor="@color/fontBlack1"
        android:textSize="32px" />

    <TextView
        android:id="@+id/export_shi_temperature"
        android:layout_width="160px"
        android:layout_height="wrap_content"
        android:text="温度"
        android:textAlignment="center"
        android:textColor="@color/fontBlack1"
        android:textSize="32px" />

    <TextView
        android:id="@+id/export_shi_resulttype"
        android:layout_width="180px"
        android:layout_height="wrap_content"
        android:text="结果类型"
        android:textAlignment="center"
        android:textColor="@color/fontBlack1"
        android:textSize="32px" />

    <TextView
        android:id="@+id/export_shi_result"
        android:layout_width="240px"
        android:layout_height="wrap_content"
        android:text="测试结果"
        android:textAlignment="center"
        android:textColor="@color/fontBlack1"
        android:textSize="32px" />

    <TextView
        android:id="@+id/export_shi_creator"
        android:layout_width="200px"
        android:layout_height="wrap_content"
        android:text="测试人"
        android:textAlignment="center"
        android:textColor="@color/fontBlack1"
        android:textSize="32px" />
</LinearLayout>
