                        android:layout_marginTop="@dimen/_10px" />


                    <com.jh.automatic_titrator.ui.data.method.view.ParamsListItemView
                        android:id="@+id/titrator_extra_params_list_layout"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_10px" />
                </LinearLayout>
            </LinearLayout>
        </ScrollView>

    </LinearLayout>

